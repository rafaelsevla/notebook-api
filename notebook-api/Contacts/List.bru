meta {
  name: List
  type: http
  seq: 1
}

get {
  url: {{API_URL}}/v1/contacts?page[number]=1&page[size]=5
  body: none
  auth: inherit
}

params:query {
  page[number]: 1
  page[size]: 5
}

headers {
  Accept: application/json
  If-None-Match: {{ETAG}}
}

script:post-response {
  const etag = res.getHeader('etag')
  
  bru.setEnvVar("ETAG", etag)
  
}

settings {
  encodeUrl: true
  timeout: 0
}
